{
  "name": "db-simulator-ui",
  "version": "1.0.0",
  "description": "Electron frontend for DB Simulator",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack --watch\" \"electron .\" \"cd ../python && python main.py api\"",
    "build": "cross-env NODE_ENV=production webpack && electron-builder",
    "clean": "rimraf dist",
    "webpack": "webpack"
  },
  "author": "FX93V2",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.3",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.1",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.0",
    "chart.js": "^4.4.0",
    "date-fns": "^4.1.0",
    "electron-store": "^8.1.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.52.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.16.0",
    "react-split-pane": "^0.1.92",
    "reactflow": "^11.10.1",
    "sass": "^1.69.0",
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.7"
  },
  "build": {
    "appId": "com.fx93v2.db-simulator",
    "productName": "DB Simulator",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "index.html"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
